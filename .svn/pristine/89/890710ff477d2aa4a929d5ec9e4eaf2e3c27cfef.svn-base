.citypanel{width:700px;background:#fff;border:1px solid #abadb3;padding:5px;font-size:14px;display:none; position:absolute; z-index:10000;}
.citypanel .tip{ position:relative; padding:5px;}
.citypanel .tip img{ position:absolute; top:0px; right:0px;cursor:pointer;}
.citypanel .tip a{position:absolute;top:0px;right:0px;cursor:pointer;width:20px;height:20px;display:inline-block;background:url(../images/btn_close.png) no-repeat 3px -57px;}
.citypanel .tip a:hover{background-position:3px -83px;}
.citypanel ul{padding:0;border:0;margin:0; list-style:none;}
.citypanel .tab-nav{position:relative;padding:3px 5px 0;height:32px;line-height:32px;zoom:1;z-index:5;}
.citypanel .tab-nav li{float:left;border:1px solid #fff;border-bottom:#bec2c4;background:#fff;padding:0 10px; margin-right:3px;height:30px;line-height:30px;cursor:pointer;font-size:14px;font-style:normal;}
.citypanel .tab-nav li.selected{border-color:#b3c2c4;border-bottom:1px solid #fff;background:#fff;}
.citypanel .tab-content{border-top:1px solid #b3c2c4;margin-top:-1px;padding:10px 0px;line-height:22px;position:relative;zoom:1;}
.citypanel .tab-content dl{}
.citypanel .tab-content dl:after{clear:both;content:" ";display:block;height:0;visibility:hidden;}
.citypanel .tab-content dt{float:left;display:block;padding-left:3px;color:#0195ff;text-indent:5px;line-height:32px;font-size:14px;}
.citypanel .tab-content dd{display:block; overflow:hidden;margin-left:30px;zoom:1;}
.citypanel .tab-content dd a{float:left;padding-left:4px;width:66px;height:30px;line-height:30px;text-align:left;color:#333;border:1px solid #fff;text-decoration:none;overflow:hidden;}
.citypanel .tab-content dd a:hover{background:#0195ff;border-color:#0185ff;text-decoration:none;color:#ffffff;}