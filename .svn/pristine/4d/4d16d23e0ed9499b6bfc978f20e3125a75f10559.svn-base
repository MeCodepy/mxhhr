	margin-top:13px;}

.FlListMain .ListBox .ListLeft .search-promotions .flight-table .flight-iu-detail li .flight-detali{
	float:left;
	width:700px;
	margin-bottom:7px;}
	
.FlListMain .ListBox .ListLeft .search-promotions .flight-table .flight-detali hr {
    width: 700px;
	border-bottom: 1px solid #ececec !important;
