/******************************  底部样式   **************************************/
.page-footer{
	clear: both;
    background: #fff;
    padding-bottom: 12px;
    padding-top: 12px;}

.page-footer .clearfix{
	text-align:center;}

.page-footer .clearfix .links-list li{
	    margin: 0;
    text-decoration: none;
    border-right: 1px solid #969696;
    padding-left: 50px;
    text-align: center;
    padding-right: 50px;
    line-height: 50px;
    display: inline;
    color: #969696;}

.page-footer .clearfix .links-list li .ng-binding{
	color: #969696;
    text-decoration: none;
    font-weight: 400;}

.page-footer .clearfix p{
	text-align: center;
    line-height: 1.3;
    padding-top: 5px;
    margin: 0;
    color: #969696;}


