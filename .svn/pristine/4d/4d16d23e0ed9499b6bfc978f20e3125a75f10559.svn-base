}

.FlListMain .ListBox .ListLeft .search-promotions .flight-table .flight-iu-detail li .flightPrice span em{
	font-size:22px;
	color:#ff5452;
	font-weight:bold;}

.FlListMain .ListBox .ListLeft .search-promotions .flight-table .flight-iu-detail li .flightPrice .bookBtn{
	width:110px;
	height:30px;
	line-height:30px;
	display:block;
	color:#FFF;
	margin-left:33px;
	border-radius:20px;
	background:#0195ff;
	text-align:center;
	float:left;
	cursor:pointer;}

.FlListMain .ListBox .ListLeft .search-promotions .more-detail{
	color:#0195ff;
	position:relative;
	top:-16px;
	left:64px;
	cursor:pointer;}

#more-detail{
	display:none;
	float:left;
	background:#FFF;
	padding:20px 30px;
	width:820px;
	border-top:1px dashed #d9d9d9;}

#more-detail .detail-top-tip{
	font-size:14px;
	color:#333;
	margin-bottom:20px;
	font-weight:bold;}

#more-detail .detail-top-tip span{
	margin-right:30px;}

#more-detail .detail-top-tip em{
	float:right;}

#more-detail .flight_fr{
	float:left;
	width:700px;
	line-height:20px;}

#more-detail .flight_fr .fl{
	float:left;}

