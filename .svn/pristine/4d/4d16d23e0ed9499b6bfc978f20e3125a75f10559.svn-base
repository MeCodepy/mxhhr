@charset "utf-8";
/* 机票查询列表页 */
body{
	background:#f3f2f5;}
.wapper{
	width:1190px;
	margin:0 auto;}
/******************************  头部样式   **************************************/
.topNav{
	width:99%;
	padding:0 0.5%;
	background:#000;
	height:50px;
	position:fixed;
	top:0;
	left:0;
	z-index:10;}

.topNav .navLeft{
	width:33%;
	float:left;}

.topNav .navLeft .nav-ui li{
	float:left;
	line-height:47px;
	font-size:20px;
	color:#fff;
	margin-right:20px;
	padding:0 10px;
	cursor:pointer;}

.topNav .navLeft .nav-ui li:hover{
	border-bottom:3px solid #0195ff;}

.topNav .navLeft .nav-ui .nav{
	border-bottom:3px solid #0195ff;}

.topNav .navcont{
	width:33%;
	float:left;
	text-align:center;}

.topNav .navRight{
	width:33%;
	float:left;}

.topNav .navRight ul li{
	float:right;
	line-height:50px;
	font-size:20px;
	color:#fff;}

/********* 主内容 **********/
.FlListMain{
	background:#f3f2f5;
	width:100%;}

.FlListMain .listSearch{
	height:68px;
	width:1148px;
	border:1px solid #ececec;
	padding:20px;
	background:#FFF;}

.FlListMain .listSearch .xingcheng{
	width:80px;
	margin-right:20px;
	float:left;
	border-right:1px solid #ccc;}

.FlListMain .listSearch .xingcheng li{
	margin-right:30px;
	cursor:pointer;}

.FlListMain .listSearch .xingcheng li em{
	display:block;
	float:left;
	margin-top:3px;
	margin-right:7px;
	width:8px;
	height:8px;
	border-radius:50%;
	border:2px solid #0195ff;}

.FlListMain .listSearch .xingcheng li:hover em{
	background:#28a1f8;}

.FlListMain .listSearch .xingcheng li.active em{
	background:#28a1f8;}

.FlListMain .listSearch .search-wap{
	width:1027;
	float:left;}
	
.FlListMain .listSearch .search-wap label{
	color:#999;}

.FlListMain .listSearch .search-wap .eye-dropdown-select{
	width:90px;
	margin-left:5px;
	margin-right:60px;
	border: 1px solid #D9D9D9;}

.FlListMain .listSearch .search-wap .flightPeople{
	margin-top:5px;}

.FlListMain .listSearch .search-wap .flight-search{
