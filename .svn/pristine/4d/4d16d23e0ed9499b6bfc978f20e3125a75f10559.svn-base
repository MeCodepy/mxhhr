.FlListMain .listSearch .search-wap .flight-search label{
	padding-right:5px;}

.FlListMain .listSearch .search-wap .flight-search input{
	border: 1px solid #D9D9D9;
    padding: 5px;
    background-color: #FFF;
    border-radius: 0px;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
	width:110px;
	margin-right:15px;}

.FlListMain .listSearch .search-btn-box{
	float:left;
	width:215px;
	padding-left:50px;}

.FlListMain .listSearch .search-btn-box .searchFlight{
	display:block;
	border-radius:20px;
	width:180px;
	height:40px;
	line-height:40px;
	font-size:14px;
	color:#fff;
	background:#0195ff;
	text-align:center;}

.FlListMain .listSearch .search-btn-box label{
	margin-left:10px;
	margin-top:10px;
	font-size:14px;
	color:#999;}
	
.FlListMain .listSearch .search-btn-box select{
	width:100px;
	margin-top:10px;}
.FlListMain .ListBox{
	margin-top:20px;}

.FlListMain .ListBox .ListLeft{
	width:898px;
	float:left;}

.FlListMain .ListBox .ListLeft .search-promotions .search-trip{
	line-height:34px;}

.FlListMain .ListBox .ListLeft .search-promotions .search-trip span{
	font-size:20px;
	font-weight: 400;
    color: #666;
	padding-right:30px;}

.FlListMain .ListBox .ListLeft .search-promotions .search-trip em{
	font-size:12px;
	color:#999;
	margin-right:20px;}

.FlListMain .ListBox .ListLeft .search-promotions .search-choose{
