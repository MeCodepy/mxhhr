	background: url(../images/bg_transit.gif) no-repeat scroll center transparent;
    line-height: 22px;
    padding-left: 80px;
    margin: 17px 0;
	width:650px;
	float:left;}

#more-detail .flight_fr  .flight_transit .fr{
	float:right;}

