 /* 顶部 */
.topBox {
	border-bottom:#dddddd solid 1px;
}
.topBox .topContent {
	width:1190px;
	margin:0 auto;
	height:35px;
	overflow:hidden;
	clear:both;
}
.topBox .topContent .topText {
	line-height:35px;
	color:#666;
}
.topBox .topContent .topText a {
	padding:0px 5px;
	color:#ff6804;
}
.topBox .topContent .topText a:hover {
	color:#ff6804;
	text-decoration:underline;
}
.topBox .topContent .topText span {
	padding:0px 8px;
	color:#ff6804
}
.topBox .topContent .topLink {
	line-height:35px;
	color:#666;
	text-align:right;
	margin: 0 0 10px 0;
}
.topBox .topContent .topLink img {
	vertical-align:middle;
}
.topBox .topContent .topLink a {
	color:#666;
	padding:0px 5px;
}
.topBox .topContent .topLink a:hover {
	color:#ff6804;
}
.topBox .topContent .topLink .iconfont {
	font-size:14px;
}
.topBox .topContent .topLink .mobile {
	vertical-align:top;
}
.topBox .topContent .topLink ul li {
	float:left;
	line-height:35px;
	padding:0 6px;
}
.navBox .login-info {
	margin:0;
	height:42px;
	color:#FFF;
	line-height:17px;
	background:url(../images2014/common/foot_icon.png) no-repeat 4px center #FF9913;
}
.navBox .login-info a {
	color:#FFF;
}
.navBox .login-info a:hover {
	text-decoration:underline;
}
.navBox .login-info .login-reg, #header .login-info .mylutao {
	margin-left:40px;
}
.navBox .login-info .login-reg {
	margin-top:4px;
}
.navBox .login-info .login-reg a {
	font-size: 14px;
}
