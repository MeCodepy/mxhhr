@charset "UTF-8";
/* CSS Document */

/* *****全局样式********/

/* ---------字体图标---------*/
@font-face {
font-family: 'iconfont';
 src: url('../css/fonts/iconfont.eot'); /* IE9*/
 src: url('../css/fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */  url('../css/fonts/iconfont.woff') format('woff'), /* chrome、firefox */  url('../css/fonts/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/  url('../css/fonts/iconfont.svg#uxiconfont') format('svg'); /* iOS 4.1- */
}
.lt-iconfont {
	font-family:"iconfont" !important;
	font-size:16px;
	font-style:normal;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: 0.2px;
	-moz-osx-font-smoothing: grayscale;
}
/** 设置默认字体 **/
body, button, input, select, textarea, ul, li {
/*	font:12px/1.5 "Microsoft Yahei", "\5FAE\8F6F\96C5\9ED1", tahoma, arial, "Hiragino Sans GB", \5b8b\4f53;*/
