                </li>
                <li class="col2">
                    <h3>${pasgsects.fltinfo.branch }</h3>
                    <span>${pasgsects.fltinfo.fltNo }</span>
                    <span></span>
                    <p><span class="tdcColor">经济舱</span></p>
                </li>
                <li class="col3">
                    <span class="scol1">
                        <h3>${pasgsects.fltinfo.deptime }</h3>
                        <p>${pasgsects.fltinfo.depname }</p>
                    </span>
                    <span class="scol2">
                         <i class="time">约${my:getFlightTime(pasgsects.fltinfo.deptime,pasgsects.fltinfo.arrtime) }</i> 
                    </span>
                    <span class="scol3">
                        <h3>${pasgsects.fltinfo.arrtime }</h3>
                        <p>${pasgsects.fltinfo.arrname }</p>
                    </span>
                </li>
            </ul>
            <div style="clear:both;"></div>
            <div class="line"></div>
            </c:forEach>
            </c:forEach>
